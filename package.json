{
  "name": "inprogress-proto",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run _server & npm run _build-dev",
    "debug": "npm run _server & npm run _build-debug ",
    "_server": "live-server --host=192.168.1.102 --port=3000 --ignore=src --no-browser",
    "build": "NODE_ENV=production browserify src/main.jsx --extension=jsx | uglifyjs --screw-ie8 -c=dead_code,evaluate,loops,unused > build/bundle.js",
    "_build-dev": "NODE_ENV=development watchify src/main.jsx --extension=jsx -o build/bundle.js -p [ livereactload --host 192.168.1.102 ] -dv",
    "_build-debug": "NODE_ENV=development watchify src/main.jsx --extension=jsx -o build/bundle.js -dv"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "envify"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "backbone-relational": "^0.10.0",
    "backbone.localstorage": "^1.1.16",
    "browserify": "^12.0.2",
    "envify": "^3.4.0",
    "html5sortable": "^0.3.1",
    "jquery": "^2.2.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "live-server": "^0.9.0",
    "livereactload": "^2.1.0",
    "node-static": "^0.7.7",
    "react-proxy": "^1.1.2",
    "watchify": "^3.6.1"
  }
}
